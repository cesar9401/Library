<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.NuevoPrestamoController">
    <stylesheets>
        <URL value="@/org/openjfx/style/nuevoprestamo.css" />
    </stylesheets>
   <children>
      <Label fx:id="labelCarnet" layoutX="30.0" layoutY="30.0" prefHeight="30.0" prefWidth="70.0" styleClass="labelCarnet" text="Carnet">
         <font>
            <Font size="16.0" />
         </font></Label>
      <JFXTextField fx:id="textCarnet" layoutX="100.0" layoutY="30.0" prefHeight="30.0" prefWidth="175.0" styleClass="textCarnet" />
      <Label fx:id="labelLibro" layoutX="325.0" layoutY="31.0" prefHeight="30.0" prefWidth="70.0" styleClass="labelLibro" text="Libro">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <JFXTextField fx:id="textLibro" layoutX="395.0" layoutY="30.0" prefHeight="30.0" prefWidth="175.0" styleClass="textLibro" />
      <JFXButton fx:id="buttonProcesar" layoutX="175.0" layoutY="340.0" onAction="#ActionPrestamoView" prefHeight="36.0" prefWidth="100.0" styleClass="buttonProcesar" text="Procesar">
         <font>
            <Font size="16.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="buttonCancelar" defaultButton="true" layoutX="325.0" layoutY="340.0" onAction="#ActionPrestamoView" prefHeight="36.0" prefWidth="100.0" styleClass="buttonCancelar" text="Cancelar">
         <font>
            <Font size="16.0" />
         </font>
      </JFXButton>
      <TableView fx:id="tableLibro" layoutX="30.0" layoutY="90.0" prefHeight="225.0" prefWidth="540.0" styleClass="tableLibro">
        <columns>
          <TableColumn fx:id="columnCodigo" editable="false" resizable="false" styleClass="columnCodigo" text="Codigo" />
          <TableColumn fx:id="columnNombre" editable="false" prefWidth="165.0" resizable="false" styleClass="columnNombre" text="Nombre" />
            <TableColumn fx:id="columnAutor" editable="false" prefWidth="165.0" resizable="false" styleClass="columnAutor" text="Autor" />
            <TableColumn fx:id="columnEdicion" editable="false" prefWidth="65.0" resizable="false" styleClass="columnEdicion" text="Edicion" />
            <TableColumn fx:id="columnStock" editable="false" prefWidth="65.0" resizable="false" styleClass="columnStock" text="Stock" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
